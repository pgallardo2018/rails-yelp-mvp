<div class="container">
  <h1>Restaurants</h1>
  <%= link_to 'Create New Restaurant', new_restaurant_path %>
  <% @restaurants.each do |restaurant| %>
    <h2><%= restaurant.name %></h2>
    <p><%= restaurant.address %></p>
    <%= link_to 'View Detail', restaurant_path(restaurant) %>
  <% end %>
</div>